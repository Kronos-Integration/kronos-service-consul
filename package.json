{
  "name": "kronos-service-consul",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/service-consul.js",
  "module": "src/service-consul.js",
  "description": "kronos service to integrate with consul",
  "keywords": [
    "kronos-service"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "docs": "documentation readme src/service-consul.js --section=API",
    "lint": "documentation lint src/service-consul.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "test": "ava"
  },
  "dependencies": {
    "consul": "^0.34.1",
    "koa-route": "^3.2.0",
    "kronos-service": "^4.17.2",
    "model-attributes": "^4.0.0",
    "network-address": "^1.1.2",
    "promise-repeat": "^0.4.0"
  },
  "devDependencies": {
    "ava": "^1.2.1",
    "c8": "^3.4.0",
    "documentation": "^9.1.1",
    "esm": "^3.2.4",
    "kronos-service-health-check": "^2.10.11",
    "kronos-service-koa": "^4.0.0",
    "kronos-service-manager": "^3.5.10",
    "markdown-doctest": "^0.9.1",
    "rollup": "^1.1.2",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-executable": "^1.4.4",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "semantic-release": "^15.13.3"
  },
  "engines": {
    "node": ">=10.15.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-service-consul.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-consul/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-consul#readme",
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
