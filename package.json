{
  "name": "@kronos-integration/service-consul",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/service-consul.mjs",
  "exports": {
    ".": "./src/service-consul.mjs"
  },
  "description": "kronos service to integrate with consul",
  "keywords": [
    "kronos-service"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/service-consul.mjs --section=API && documentation readme ./src/service-consul.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint src/service-consul.mjs && documentation lint ./src/service-consul.mjs"
  },
  "dependencies": {
    "@kronos-integration/endpoint": "^9.3.0",
    "@kronos-integration/service": "^10.0.1",
    "consul": "^0.39.0",
    "model-attributes": "^4.1.7",
    "promise-repeat": "^0.4.0"
  },
  "devDependencies": {
    "@kronos-integration/service-http": "^10.5.3",
    "ava": "^3.14.0",
    "c8": "^7.3.5",
    "documentation": "github:arlac77/documentation",
    "semantic-release": "^17.3.0"
  },
  "engines": {
    "node": ">=14.15.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service-consul.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service-consul/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service-consul#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-kronos-component"
    ]
  }
}
