{
  "name": "kronos-service-consul",
  "version": "0.0.0-semantic-release",
  "description": "kronos service to integrate with consul",
  "main": "dist/module.js",
  "module": "src/ServiceConsul.js",
  "keywords": [
    "kronos-service"
  ],
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*-test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "pretest": "rollup -c",
    "posttest": "markdown-doctest",
    "precover": "rollup -c",
    "prepare": "rollup -c"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "devDependencies": {
    "chai": "^4.1.2",
    "jsdoc-to-markdown": "^3.0.0",
    "kronos-service-health-check": "^2.10.11",
    "kronos-service-koa": "^3.3.12",
    "kronos-service-manager": "^3.5.10",
    "markdown-doctest": "^0.9.1",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "rollup": "^0.50.0",
    "semantic-release": "^8.2.2"
  },
  "dependencies": {
    "consul": "^0.30.0",
    "koa-route": "^3.2.0",
    "kronos-service": "^4.8.2",
    "model-attributes": "^3.0.10",
    "network-address": "^1.1.2",
    "promise-repeat": "^0.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-service-consul.git"
  },
  "engines": {
    "node": ">=8.7.0"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-consul/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-consul#readme",
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  }
}
