{
  "name": "kronos-service-consul",
  "version": "0.0.0-semantic-release",
  "description": "kronos service to integrate with consul",
  "main": "dist/module.js",
  "module": "src/ServiceConsul.js",
  "keywords": [
    "kronos-service"
  ],
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepublish": "rollup --output=dist/module.js -c -- src/ServiceConsul.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^2.0.0",
    "jsdoc-to-markdown": "^3.0.0",
    "kronos-service-health-check": "^2.10.8",
    "kronos-service-koa": "^3.3.5",
    "kronos-service-manager": "^3.5.8",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "rollup": "^0.41.4",
    "semantic-release": "^6.3.6"
  },
  "dependencies": {
    "consul": "0.28.0",
    "koa-route": "^3.2.0",
    "kronos-service": "^4.5.4",
    "model-attributes": "^3.0.7",
    "network-address": "1.1.2",
    "promise-repeat": "^0.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-service-consul.git"
  },
  "engines": {
    "node": ">=6.10.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-consul/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-consul#readme",
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  }
}