    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "doc": "node ./node_modules/.bin/jsdoc lib/*.js"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "devDependencies": {
    "chai": "3.5.0",
    "cz-conventional-changelog": "1.2.0",
    "istanbul": "0.4.5",
    "jsdoc": "3.4.0",
    "kronos-service-health-check": "^2.9.17",
    "kronos-service-koa": "^3.0.1",
    "kronos-service-manager": "^3.4.14",
    "mocha": "3.0.2",
    "semantic-release": "6.3.0"
  },
  "dependencies": {
    "consul": "0.25.0",
    "koa-route": "3.1.0",
