{
  "name": "kronos-service-consul",
  "version": "0.0.0-semantic-release",
  "description": "kronos service to integrate with consul",
  "main": "dist/module.js",
  "module": "src/ServiceConsul.js",
  "keywords": [
    "kronos-service"
  ],
  "scripts": {
    "cover": "npm run prepublish && istanbul cover node_modules/.bin/_mocha -- -u exports tests/*_test.js",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js > README.md",
    "prepublish": "rollup --output=dist/module.js -c -- src/ServiceConsul.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^2.0.1",
    "kronos-service-health-check": "^2.10.2",
    "kronos-service-koa": "^3.0.7",
    "kronos-service-manager": "^3.4.20",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.2.0",
    "rollup": "^0.39.2",
    "semantic-release": "^6.3.5"
  },
  "dependencies": {
    "consul": "0.27.0",
    "koa-route": "^3.2.0",
    "kronos-service": "^4.3.1",
    "model-attributes": "^3.0.1",
    "network-address": "1.1.0",
    "promise-repeat": "^0.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-service-consul.git"
  },
  "engines": {
    "node": ">=6.9.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-consul/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-consul#readme"
}